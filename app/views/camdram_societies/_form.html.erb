<%= form_with model: @camdram_society, local: true do |form| %>
  <div class="form-group row">
    <%= form.label :slack_webhook, class: 'col-sm-2 col-form-label' %>
    <div class="col-sm-10">
      <%= form.text_field :slack_webhook, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group row mt-2">
    <div class="col-sm-10">
      <%= form.submit 'Save', class: 'btn btn-outline-primary' %>
    </div>
  </div>
<% end %>

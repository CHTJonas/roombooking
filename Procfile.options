app_name: roombooking
log_root: log
pid_root: tmp/pids
console_command: bundle exec rails console
env:
  RAILS_ENV: production
processes:
  web:
    log_file_name: unicorn.log
    restart_mode: usr2
    term_signal: QUIT
  worker:
    log_file_name: sidekiq-worker.log
    restart_mode: start-term
    term_signal: TERM
  mailer:
    log_file_name: sidekiq-mailer.log
    restart_mode: start-term
    term_signal: TERM
